{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": [2, "single"],
    "semi": [2, "always"]
  },
  "globals": {
    "describe": false,
    "beforeEach": false,
    "afterEach": false,
    "it": false,
    "expect": false
  }
}